
Vagrant.configure(2) do |config|

  config.vm.box = "robwc/minitrusty64"
  config.vm.hostname = 'aos-ztp'

  config.vm.provider "virtualbox" do |vb|
    vb.name = 'aos-ztp'
    vb.gui = true
    vb.memory = "512"
  end

  config.vm.provision "ansible" do |ansible|
    ansible.playbook = "aos-ztp-ansible.yml"
  end


end
